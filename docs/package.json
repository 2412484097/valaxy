    "docs:preview": "vitepress preview"
  },
  "dependencies": {
    "valaxy": "link:../packages/valaxy",
    "valaxy-addon-algolia": "link:../packages/valaxy-addon-algolia",
    "valaxy-theme-press": "link:../packages/valaxy-theme-press"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "vitepress": "1.0.0-beta.5"
  }
}
